package com;

import org.hibernate.Session;
import org.hibernate.SessionFactory;

public class TestMain {

	public static void main(String[] args) {

		SessionFactory sessionFactory =  HibernateUtil.getSessionFactory();

		Session session = sessionFactory.openSession();

		Employee stu = new Employee();
		stu.setId(1);
		stu.setName("ABC1");
		stu.setAddress("Pune1");

		
		Employee emp2 = new Employee();
		emp2.setId(2);
		emp2.setName("ABC2");
		emp2.setAddress("Pune2");
		
		
		session.save(stu);
		session.beginTransaction().commit();
		

		Employee emp =session.get(Employee.class, 1);
		System.out.println("Retrieved Employee from Get: ,"+emp);
		

		Employee emp1 =session.load(Employee.class, 1);
		System.out.println("Retrieved Employee through Load: ,"+emp1);
		
		session.close();
		sessionFactory.close();
		System.out.println("record Inserted ....");

	}
}
